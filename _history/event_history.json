---
title: Event History Data
---
{
  "title": {
    "background": {
      "color": "#212121"
    },
    "media": {
      "url": "{{ site.logo | prepend: "img/" | prepend: sitebase.url | prepend: site.url }}",
      "caption": "",
      "credit": "{{ site.name }}"
    },
    "text": {
      "headline": "Event History<br/> 1935 - present",
      "text": "<p>{{ site.title }}</p>"
    }
  },
  "events": [{% for event in site.data.rendezvous %}
    {
      "media": {
        "url": "",
        "caption": "",
        "credit": ""
      },
      "start_date": {
        "month": "{{ event.start_date | date: "%m" }}",
        "day": "{{ event.start_date | date: "%-d" }}",
        "year": "{{ event.start_date | date: "%Y" }}"
      },
      {% if event.end_date != null %}
      "end_date": {
        "month": "{{ event.end_date | date: "%m" }}",
        "day": "{{ event.end_date | date: "%-d" }}",
        "year": "{{ event.end_date | date: "%Y" }}"
      },
      {% endif %}
      "text": {
        "headline": "{{ event.theme | xml_escape }}",
        "text": "<p>{{ event.location }}<br />{{ event.address }}<br />{{ event.city_state_zipcode }}</p>{% if event.price1 != null %}<p>Registration: {{ event.price1 }}</p>{% endif %}{% if event.price2 != null %}<p>Registration: {{ event.price2 }}</p>{% endif %}{% if event.membership != null %}<p>Membership Dues: {{ event.membership }}</p>{% endif %}"
      }
    }{% unless forloop.last %},{% endunless %}{% endfor %}
  ]
}