{  
  "@context":"http://schema.org",
  "@type":"Organization",
  "logo":"{{ site.logo | prepend: site.baseurl | prepend: site.url }}",
  "name":"{{ site.title }}",
  "legalName":"{{ site.legal_name }}",
  "description":"{{ site.mission }}",
  "foundingDate":"1935",
  "areaServed":"Colorado",
  "url":"{{ page.url | prepend: site.baseurl | prepend: site.url }}",
  "sameAs":[
    "{{ site.baseurl | prepend: site.url }}",{% for media in site.social %}
    "{{ media.url}}"{% unless forloop.last %},{% endunless %}{% endfor %}
  ],
  "subOrganization":[  
    "COABE",
    "MPAEA"
  ],
  "event":[
    {
      "@type":"Event",
      "name":"{{ page.title }}",
      "alternateName":"{{ page.theme }}",
      "startDate":"{{ page.start_date }}",
      "endDate":"{{ page.end_date }}",
      "image":"{{ page.image_path | prepend: "/img/events/" | prepend: site.baseurl | prepend: site.url }}",
      "sameas":{% if page.event_url != null %}[
        "{{ page.url | prepend: site.baseurl | prepend: site.url }}",
        "{{ page.event_url | prepend: site.baseurl | prepend: site.url }}"
      ],{% else %}"{{ page.url | prepend: site.baseurl | prepend: site.url }}",{% endif %}
      "location":{
        "@type":"Place",
        "name":"{{ page.venue.name }}",
        "sameAs":"{{ page.venue.url }}",
        "address":{
          "@type":"PostalAddress",
          "streetAddress":"{{ page.venue.address }}",
          "addressLocality":"{{ page.venue.city }}",
          "addressRegion":"{{ page.venue.state }}",
          "postalCode":"{{ page.venue.zipcode }}"
        }
      },
      "offers":{
        "@type":"AggregateOffer",
        "lowPrice":"{{ page.offers.price1 }}",
        "highPrice":"{{ page.offers.price2 }}",
        "priceCurrency":"USD",
        "url":"{{ page.offers.payment_url }}"
      }
    }
  ]
}