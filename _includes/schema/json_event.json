{  
  "@context":"http://schema.org",
  "@type":"Organization",
  "logo":"{{ "/img/logo.png" | prepend: site.baseurl | prepend: site.url }}",
  "name":"{{ site.title }}",
  "legalName":"{{ site.legal_name }}",
  "description":"{{ site.mission }}",
  "foundingDate":"1935",
  "areaServed":"Colorado",
  "url":"{{ site.baseurl | prepend: site.url }}",
  "subOrganization":[
    "COABE",
    "MPAEA"
  ],
  "sameAs":[{% for social_media in site.social_media %}
    "{{ social_media.url}}"{% unless forloop.last %},{% endunless %}{% endfor %}
  ],
  "event":[
    {
      "@type":"Event",
      "name":"{{ page.title }}",
      "alternateName":"{{ page.theme }}",
      "startDate":"{{ page.date }}",
      "endDate":"{{ page.end_date }}",
      "sameas":"{{ page.url }}",
      "location":{
        "@type":"Place",
        "name":"{{ venue.location }}",
        "sameAs":"{{ venue.url }}",
        "address":{
          "@type":"PostalAddress",
          "streetAddress":"{{ venue.location }}",
          "addressLocality":"{{ venue.city }}",
          "addressRegion":"{{ venue.state }}",
          "postalCode":"{{ venue.zipcode }}"
        }
      },
      "offers":{
        "@type":"AggregateOffer",
        "lowPrice":"{{ page.price1 }}",
        "highPrice":"{{ page.price2 }}",
        "priceCurrency":"USD",
        "url":"{{ page.payment_online }}"
      }
    }
  ]
}