{  
  "@context":"http://schema.org",
  "@type":"Organization",
  "logo":"{{ "/img/logo.png" | prepend: site.baseurl | prepend: site.url }}",
  "name":"{{ site.name }}",
  "legalName":"{{ site.legal_name }}",
  "description":"{{ site.mission }}",
  "foundingDate":"1935",
  "areaServed":"Colorado",
  "url":"{{ site.url }}",
  "subOrganization":[  
    "COABE",
    "MPAEA"
  ],
  "sameAs":[  
    "https://www.linkedin.com/company/colorado-adult-education-professional-association-caepa-",
    "https://www.facebook.com/caepa.co",
    "https://twitter.com/caepa_co",
    "http://github.com/caepa"
  ],
  "event":[  
    {  
      "@type":"Event",
      "name":"{{ page.title }}",
      "alternateName":"{{ page.theme }}",
      "startDate":"{{ page.date }}",
      "endDate":"{{ page.end_date }}",
      "sameas":"{{ page.url }}",
      "location":{  
        "@type":"Place",
        "name":"{{ venue.location }}",
        "sameAs":"{{ venue.url }}",
        "address":{  
          "@type":"PostalAddress",
          "streetAddress":"{{ venue.location }}",
          "addressLocality":"{{ venue.city }}",
          "addressRegion":"{{ venue.state }}",
          "postalCode":"{{ venue.zipcode }}"
        }
      },
      "offers":{  
        "@type":"AggregateOffer",
        "lowPrice":"{{ page.price1 }}",
        "highPrice":"{{ page.price2 }}",
        "priceCurrency":"USD",
        "url":"{{ page.payment_online }}"
      }
    }
  ]
}